<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
android:layout_height="match_parent"
   android:paddingLeft="@dimen/activity_horizontal_margin"
android:paddingRight="@dimen/activity_horizontal_margin"
android:paddingTop="@dimen/activity_vertical_margin"
android:paddingBottom="@dimen/activity_vertical_margin"
tools:context=".Log_In"
android:id="@+id/W1"
android:onClick="login"
android:background="@android:drawable/ic_lock_idle_low_battery"
android:backgroundTint="#ff5257ff"
android:backgroundTintMode="src_over">

<Button
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="LogIn"
    android:id="@+id/but1"
    android:onClick="login"
    android:background="#ff5fff32"
    android:textColor="@android:color/black"
    android:layout_alignParentBottom="true"
    android:layout_centerHorizontal="true" />

<EditText
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/UN"
    android:layout_alignParentTop="true"
    android:layout_alignLeft="@+id/but1"
    android:layout_alignStart="@+id/but1"
    android:layout_marginTop="129dp"
    android:layout_alignParentRight="true"
    android:layout_alignParentEnd="true" />

<EditText
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:inputType="textPassword"
    android:ems="10"
    android:id="@+id/PW"
    android:layout_centerVertical="true"
    android:layout_alignParentRight="true"
    android:layout_alignParentEnd="true" />

<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="User_Name"
    android:id="@+id/textView1"
    android:textSize="20dp"
    android:layout_alignBottom="@+id/UN"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true"
    android:textColor="@android:color/black" />

<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="Pass_Word"
    android:id="@+id/textView2"
    android:textSize="20dp"
    android:layout_alignBottom="@+id/PW"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true"
    android:textColor="@android:color/black" />

</RelativeLayout>